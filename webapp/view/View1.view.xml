<core:View xmlns:core="sap.ui.core" controllerName="gst.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<!--This is logo of Entune-->
				<Image src="img/Entune.jpg" width="20%"></Image>
				<!--This is Menu Bar Starting-->
				<tnt:ToolHeader >
					<ToolbarSpacer width="100px"/>
					<Link text="About Us" press="linkPress" target="_blank" href="http://www.entune.co/?page_id=14" emphasized="true" >
						<layoutData>
							<OverflowToolbarLayoutData priority="Low" minWidth="15%"/>
						</layoutData>
					</Link>
					<Link text="Services" press="linkPress" target="_blank" href="http://www.entune.co/?page_id=50"  emphasized="true">
						<layoutData>
							<OverflowToolbarLayoutData priority="Low" minWidth="15%"/>
						</layoutData>
					</Link>
					<Link text="Partnership" press="linkPress" target="_blank" href="http://www.entune.co/?page_id=294"  emphasized="true">
						<layoutData>
							<OverflowToolbarLayoutData priority="Low" minWidth="15%"/>
						</layoutData>
					</Link>
					<Link text="Post" press="linkPress" target="_blank" href="http://www.entune.co/?page_id=16"  emphasized="true">
						<layoutData>
							<OverflowToolbarLayoutData priority="Low" minWidth="15%"/>
						</layoutData>
					</Link>
					<Link text="Career" press="linkPress" target="_blank" href="http://www.entune.co/?page_id=602"  emphasized="true">
						<layoutData>
							<OverflowToolbarLayoutData priority="Low" minWidth="10%"/>
						</layoutData>
					</Link>
					<Link text="Contact Us" press="linkPress" target="_blank" href="http://www.entune.co/?page_id=18"  emphasized="true">
						<layoutData>
							<OverflowToolbarLayoutData priority="Low" minWidth="15%"/>
						</layoutData>
					</Link>
				</tnt:ToolHeader>
				<!--This is Menu Bar Ends-->
				<!--The login content starts-->
				<content >
					<VBox>
						<f:SimpleForm layout="GridLayout" width="60%">
							<f:content >
								<Label text="Enter Username " design="Bold"/>
								<Input width="70%"/>
								<Link text="Forget Username" press="handleLinkPress" emphasized="true"/>
								<Label text="Enter Password " design="Bold"/>
								<Input width="70%"/>
								<Link text="Forget password" press="handleLinkPress" emphasized="true"/>
								<Label text=""/>
								<Button width="10rem"  text="Login" type="Emphasized"/>
								<Link text="Register" press="handleLinkPress" emphasized="true" />
							</f:content>
						</f:SimpleForm>
					</VBox>
				</content>
				<!--The login content starts-->
				<!--Footer starts-->
				<footer>
					<Bar>
						<contentRight>
							<Link text="Mail@Entune.co" press="handleEmailPress" target="_blank"  emphasized="true"/>
						</contentRight>
						<contentLeft>
							<Image src="img/logo.png" width="65%"/>
						</contentLeft>
					</Bar>
				</footer>
				<!--Footer ends-->
			</Page>
		</pages>
	</App>
</core:View>